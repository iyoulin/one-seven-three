local Http = game:GetService("HttpService")
local TPS = game:GetService("TeleportService")
local Api = "https://games.roblox.com/v1/games/"

local _place = game.PlaceId
local _servers = Api.._place.."/servers/Public?sortOrder=Desc&limit=100"

function ListServers(cursor)
    local Raw = game:HttpGet(_servers .. ((cursor and "&cursor="..cursor) or ""))
    return Http:JSONDecode(Raw)
end

local function FindServerWithPlayerCount(minPlayers, maxPlayers)
    local cursor
    repeat
        local Servers = ListServers(cursor)
        for _, server in ipairs(Servers.data) do
            local playerCount = server.playing
            if playerCount >= minPlayers and playerCount <= maxPlayers then
                return server
            end
        end
        cursor = Servers.nextPageCursor
    until not cursor
    return nil
end

local OptimalServer = FindServerWithPlayerCount(14, 17)

if OptimalServer then
    TPS:TeleportToPlaceInstance(_place, OptimalServer.id, game.Players.LocalPlayer)
else

    local Server, Next
    repeat
        local Servers = ListServers(Next)
        Server = Servers.data[1]
        Next = Servers.nextPageCursor
    until Server
    TPS:TeleportToPlaceInstance(_place, Server.id, game.Players.LocalPlayer)
end
